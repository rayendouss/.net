@model IEnumerable<Pidev.data.tp_jsf_tache>

@{
    ViewBag.Title = "Listtache";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>List des taches du projet </h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EndDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StartDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.desct)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nomt)
        </th>
   
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.EndDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StartDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.desct)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nomt)
        </td>
      
    </tr>
}

</table>
