@model IEnumerable<Pidev.data.tp_jsf_reclamation>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/Employes.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Ajouter une réclamation", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.description)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>

        

        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.description)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
     
        <td>

         
            @Html.ActionLink("Delete", "Delete", new { id = item.idRec })

        </td>
    </tr>
}

</table>
